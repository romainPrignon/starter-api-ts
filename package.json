{
  "name": "starter-api-ts",
  "version": "0.0.0",
  "description": "starting point of a new api",
  "private": true,
  "engines": {
    "node": "^8",
    "npm": "^5.3"
  },
  "scripts": {
    "build": "tsc",
    "release": "./scripts/release.sh",
    "start": "node src/server.js",
    "test": "jest || true",
    "version:current": "./scripts/current-version.sh"
  },
  "dependencies": {
    "body-parser": "latest",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "lodash": "latest"
  },
  "devDependencies": {
    "husky": "0.15.0-beta.16",
    "jest": "latest",
    "nodemon": "latest"
  },
  "husky": {
    "hooks": {
      "precommit": "npm install && npm test",
      "postcommit": "make release"
    }
  }
}
